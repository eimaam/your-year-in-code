{
    "version": 2,
    "name": "your-year-in-code-api",
    "builds": [
        {
            "src": "src/app.ts",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "src/app.ts"
        }
    ],
    "env": {
    "NODE_ENV": "production"
  }
}
